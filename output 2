public class Alprotugas3 {

	public static void main(String[] args) {
		int n = 6;
		int i, j;
		for (i = 1; i <= n ; i++) {
		for (i = 0; i < n ; i++) {
			for (j = n; j > i; j--) {
				System.out.print("");
			}
			for (j = 1; j <= i; j++) {
				System.out.print(j);
			}
			System.out.println();
			}
		}
	}
}
